<h1>Listing ref_insurances</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ref_insurances.each do |ref_insurance| %>
      <tr>
        <td><%= ref_insurance.name %></td>
        <td><%= link_to 'Show', ref_insurance %></td>
        <td><%= link_to 'Edit', edit_ref_insurance_path(ref_insurance) %></td>
        <td><%= link_to 'Destroy', ref_insurance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ref insurance', new_ref_insurance_path %>
