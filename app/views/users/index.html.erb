<div class="container">
  <div class="row">
    <h3>Users</h3>
    <%= link_to Devise.friendly_token, new_registration_path(:user) %>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
